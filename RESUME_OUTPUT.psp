  <body>
  
    <div class="container"><!-- main page container -->
      <div class="row">

        <!-- front-page holds all the conent on the right of the page -->
    <div class="front-page system-serif system-sans-serif span8 offset2">

<!-- STUDENT USER PERSONAL DATA -->
<section id="contactinfo">
        <%PAGE_USERS_CURSOR(RESUMATOR_ID_VAL, EMAIL_IN);%>
        </section>



<!-- RESUME CONTENT BEGINS -->
        <section id="objective">
          <!-- user objective section -->
          <p class="well well-small">Resumes shouldn't be written in one day, updating information shouldn't be a hassle, and sharing should be simple with no account required to view. Resumator3000 does all this and handles layout, typography, design, and more. We are not a resume builder; we are your resume partner dedicated to working with you over your student career. Join and start building a better resume today. <br />
           <a href="#objective-modal" data-toggle="modal"><i class="icon-pencil"> </i></a> </p>
        </section>
          <hr> 



<!-- EDUCATION SECTION -->
<!-- EDUCATION CURSOR DECLARATION -->

          <section id="education">
            <h4>Education: <a href="#add-education-modal" data-toggle="modal"><i class="icon-plus"> </i></a>  
                <a href="#edit-education-modal" data-toggle="modal"><i class="icon-pencil"> </i></a></li>
            </h4>
      <%PAGE_EDUCATION_CURSOR(RESUMATOR_ID_VAL);%>
          </section>  
          <hr>

<!-- CERTIFICATIONS AND LICENSES -->
<!-- POPULATE CERTIFICATIONS-->


          <section id="certifications">
            <h4>Certifications and Licenses: <a href="#add-certifications-modal" data-toggle="modal"><i class="icon-plus"> </i></a> <a href="#edit-certifications-modal" data-toggle="modal"><i class="icon-pencil"> </i></a> </h4>
      <%PAGE_CERTIFICATION_CURSOR(RESUMATOR_ID_VAL);%>
          </section>
          <hr>  


<!-- RESEARCH SECTION -->
<!-- POPULATE RESEARCH-->

          <section id="research">
            <h4>Research, Publications and Presentations: <a href="#add-research-modal" data-toggle="modal"><i class="icon-plus"> </i></a> <a href="#edit-research-modal" data-toggle="modal"><i class="icon-pencil"> </i></a> </h4>
              <!-- loop through all listed projects -->
      <%PAGE_PUBLICATION_CURSOR(RESUMATOR_ID_VAL);%>
          </section>
          <hr> 

<!-- PROJECTS SECTION -->
    <section id="projects">
      <h4>Projects: <a href="#add-project-modal" data-toggle="modal"><i class="icon-plus"> </i></a> <a href="#edit-project-modal" data-toggle="modal"><i class="icon-pencil"> </i></a> </h4>
  <!-- loop through all listed projects -->
      <%PAGE_PROJECT_CURSOR(RESUMATOR_ID_VAL);%>
    </section>
  <hr>  

<!-- ACADEMIC COURSEWORK -->
   <section id="academic">
      <h4>Academic Coursework: <a href="#add-academic-course-modal" data-toggle="modal"><i class="icon-plus"> </i></a> <a href="#edit-academic-course-modal" data-toggle="modal"><i class="icon-pencil"> </i></a> </h4>
         <div class="row"><!-- academic coursework row -->
      <%PAGE_COURSE_WORK_CURSOR(RESUMATOR_ID_VAL);%>
        </div><!-- end academic coursework row -->

    </section>
  <hr>  
<!-- STANDARD SKILLS -->


          <section id="skills">
            <h4>Skills: <a href="#add-skills-modal" data-toggle="modal"><i class="icon-plus"> </i></a> <a href="#edit-skills-modal" data-toggle="modal"><i class="icon-pencil"> </i></a> </h4>
            
            <div class="row"><!-- standard skills row -->
        <%PAGE_STANDARD_SKILLS_CURSOR(RESUMATOR_ID_VAL);%>
            </div><!-- end standard skills row -->
          </section>
          <hr>



<!-- TECHNICAL SKILLS -->

          <section id="technical">
            <h4>Technical Skills: <a href="#add-technical-skills-modal" data-toggle="modal"><i class="icon-plus"> </i></a> <a href="#edit-technical-skills-modal" data-toggle="modal"><i class="icon-pencil"> </i></a> </h4>

            <!-- technical row --> 

            <div class="row"><!-- techinal skills row -->
        <%PAGE_TECHNICAL_SKILLS_CURSOR(RESUMATOR_ID_VAL);%>
              </div><!-- end TECHNICAL skills row -->
          </section>
          <hr>


<!-- DESIGN SKILLS -->

          <section id="design">
            <h4>Design Skills: <a href="#add-design-skills-modal" data-toggle="modal"><i class="icon-plus"> </i></a> <a href="#edit-design-skills-modal" data-toggle="modal"><i class="icon-pencil"> </i></a> </h4>

            <!-- DESIGN row -->

            <div class="row"><!-- academic coursework row --> 
        <%PAGE_DESIGN_SKILLS_CURSOR(RESUMATOR_ID_VAL);%>
             </div><!-- end DESIGN skills row -->
          </section>
          <hr>

<!-- WORK HISTORY -->
<!--POPULATE WORK HISTORY-->

          <section id="workhistory">
            <h4>Work History: <a href="#add-work-history-modal" data-toggle="modal"><i class="icon-plus"> </i></a> <a href="#edit-work-history-modal" data-toggle="modal"><i class="icon-pencil"> </i></a> </h4>

            <div class="row"><!-- work history box -->
              <% PAGE_WORK_EXPERIENCE_CURSOR (RESUMATOR_ID_VAL); %>
            </div> <!-- end first work history row -->
          </section>
          <hr>

<!-- HONORS AND AWARDS SECTION -->
<!-- POPULATE HONORS AND AWARDS SECTION -->

          <section id="honors">
            <h4>Honors and Awards: <a href="#add-honors-modal" data-toggle="modal"><i class="icon-plus"> </i></a> <a href="#edit-honors-modal" data-toggle="modal"><i class="icon-pencil"> </i></a> </h4>
            <% PAGE_AWARD_CURSOR (RESUMATOR_ID_VAL);%>
          </section>
          <hr> 


<!-- EXTRACURRICULAR ACTIVITIES SECTION -->


          <section id="activities">
            <h4>Extra Curricular: <a href="#add-extracurricular-modal" data-toggle="modal"><i class="icon-plus"> </i></a> <a href="#edit-extracurricular-modal" data-toggle="modal"><i class="icon-pencil"> </i></a> </h4>

            <div class="row"><!-- extra curricular row -->
              <% PAGE_EXTRA_CURRICULAR_CURSOR (RESUMATOR_ID_VAL); %>
             </div><!-- extra curricular row -->
          </section>
          <hr>

<!-- REFERENCES SECTION -->

          <section id="references">
            <h4>References and Contacts: <a href="#add-references-modal" data-toggle="modal"><i class="icon-plus"> </i></a> <a href="#edit-references-modal" data-toggle="modal"><i class="icon-pencil"> </i></a> </h4>
              <% PAGE_REFERENCES_CURSOR (RESUMATOR_ID_VAL); %>
          </section>
          <hr> 

<!-- RESUME CONTENT ENDS -->


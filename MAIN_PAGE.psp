<!-- STUDENT USER PERSONAL DATA -->
<%!CURSOR USERS_CURSOR IS 

SELECT USERS.F_NAME, USERS.L_NAME, USERS.STREET, USERS.CITY, USERS.STATE, USERS.ZIPCODE, USERS.PHONE_NUMBER, USERS.ACCOUNT_TYPE
FROM USERS
WHERE USERS.RESUMATOR_ID = RESUMATOR_ID_VAL;

USERS_ROW USERS_CURSOR%ROWTYPE;%>

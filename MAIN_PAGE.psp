<!-- EDUCATION SECTION -->
<!-- EDUCATION CURSOR DECLARATION -->
<%!CURSOR ED_CURSOR
  IS
    SELECT 
      EDUCATION.SCHOOL_ID,
      EDUCATION.DEGREE,
      EDUCATION.MAJOR,
      EDUCATION.MINOR,
      EDUCATION.CERTIFICATIONS,
      EDUCATION.GPA,
      EDUCATION.GRAD_DATE,
      EDUCATION.DISPLAY_RANK
    FROM EDUCATION
    WHERE EDUCATION.RESUMATOR_ID = RESUMATOR_ID_VAL
    ORDER BY DISPLAY_RANK;
  ED_ROW ED_CURSOR%ROWTYPE;
  EDUCATION_NO_INFO EXCEPTION;
  S_NAME SCHOOL.SCHOOL_NAME%TYPE;
  S_CITY SCHOOL.SCHOOL_CITY%TYPE;
  S_STATE SCHOOL.SCHOOL_STATE%TYPE; %>
          <section id="education">
            <h4>Education: <a href="#add-education-modal" data-toggle="modal"><i class="icon-plus"> </i></a> </h4>
<!-- EDUCATION POPULATE-->
<%OPEN ED_CURSOR;
  LOOP
    FETCH ED_CURSOR INTO ED_ROW;
    SELECT SCHOOL_NAME INTO S_NAME FROM SCHOOL WHERE SCHOOL.SCHOOL_ID = ED_ROW.SCHOOL_ID;
    SELECT SCHOOL_CITY INTO S_CITY FROM SCHOOL WHERE SCHOOL.SCHOOL_ID = ED_ROW.SCHOOL_ID;
    SELECT SCHOOL_STATE INTO S_STATE FROM SCHOOL WHERE SCHOOL.SCHOOL_ID = ED_ROW.SCHOOL_ID;
    EXIT WHEN ED_CURSOR%NOTFOUND;%>
            <!-- loop through users education -->
            <ul>
                <!-- Full name of educational institution, city and state -->
                <li><a href="http://umbc.edu" target="_blank"><%=S_NAME%>, <%=S_CITY%>, <%=S_STATE%></a>
                <a href="#edit-education-modal" data-toggle="modal"><i class="icon-pencil"> </i></a></li>
                  <ul>
                    <!-- Name of Degree Program -->
                    <li><a href="http://www.is.umbc.edu" target="_blank"><%=ED_ROW.DEGREE%>, <%=ED_ROW.MAJOR%></a></li>
                      <!-- list GPA if above 3.00 -->  
                      <ul>
                        <li><%=ED_ROW.MINOR%> MINOR</li>
                        <li><%=ED_ROW.GPA%> GPA</li>
                        <li><%=ED_ROW.CERTIFICATIONS%></li>
                        <li><%=ED_ROW.GRAD_DATE%></li>
                      </ul>
                  </ul>
            </ul>
            <!-- end education loop -->  
  <%
  END LOOP;
  CLOSE ED_CURSOR;%>
          </section>  
          <hr>

<!-- CERTIFICATIONS AND LICENSES -->
<!-- POPULATE CERTIFICATIONS-->
<%! CURSOR CERT_CURSOR IS

SELECT CERTIFICATIONS.DATE_REC, CERTIFICATIONS.TITLE, CERTIFICATIONS.DISPLAY_RANK
FROM CERTIFICATIONS
WHERE CERTIFICATIONS.RESUMATOR_ID = RESUMATOR_ID_VAL
ORDER BY DISPLAY_RANK; 

CERT_COUNTER INTEGER := 0;
CERT_ROW CERT_CURSOR%ROWTYPE;
%>

          <section id="certifications">
            <h4>Certifications and Licenses: <a href="#add-certifications-modal" data-toggle="modal"><i class="icon-plus"> </i></a> <a href="#edit-certifications-modal" data-toggle="modal"><i class="icon-pencil"> </i></a> </h4>

<%OPEN CERT_CURSOR;
LOOP
FETCH CERT_CURSOR INTO CERT_ROW;
EXIT WHEN CERT_CURSOR%NOTFOUND;%>
            <div class="row"><!-- certifications and licenses row -->
            
                <!-- loop through certifications and licenses 5 max -->
                <%IF(CERT_COUNTER < 4) THEN CERT_COUNTER := CERT_COUNTER + 1;%>
                <div class="span4">
                  <ul>
                     <li><%=CERT_ROW.TITLE%>, <%=CERT_ROW.DATE_REC%></li>
                  </ul>
                </div>
                <%ELSIF(CERT_COUNTER >= 4 AND CERT_COUNTER < 9)THEN CERT_COUNTER := CERT_COUNTER + 1;%>
                <div class="span4">
                  <ul>
                     <li><%=CERT_ROW.TITLE%>, <%=CERT_ROW.DATE_REC%></li>
                  </ul>
                </div>
            </div><!-- end certifications and licenses row -->
<%
END IF;
END LOOP;
CLOSE CERT_CURSOR;%>
          </section>
          <hr>  



  AND PROJ_DESC_ID = PROJ_DESC_ID;
EXCEPTION
WHEN OTHERS THEN
  raise_application_error(-20001,'An error was encountered - ' ||SQLCODE ||' -ERROR- ' ||SQLERRM);
END; 

CREATE OR REPLACE
PROCEDURE UPDATE_PROJECTS(
    PROJECT_ID   IN VARCHAR2,
    RESUMATOR_ID IN VARCHAR2,
    PROJECT_NAME IN VARCHAR2,
    SKILL_ID     IN VARCHAR2,
    DISPLAY_RANK IN NUMBER)
AS
BEGIN
  UPDATE PROJECTS
  SET PROJECT_NAME   = PROJECT_NAME,
    DISPLAY_RANK     = DISPLAY_RANK
  WHERE RESUMATOR_ID = RESUMATOR_ID
  AND SKILL_ID       = SKILL_ID
  AND PROJECT_ID     = PROJECT_ID;
EXCEPTION
WHEN OTHERS THEN
  raise_application_error(-20001,'An error was encountered - ' ||SQLCODE ||' -ERROR- ' ||SQLERRM);
END; 

CREATE OR REPLACE
PROCEDURE UPDATE_REFERENCES(
    REFERENCE_ID IN VARCHAR2,
    RESUMATOR_ID IN VARCHAR2,
    F_NAME       IN VARCHAR2,
    L_NAME       IN VARCHAR2,
    PHONE_NUMBER IN VARCHAR2,
    ADDRESS      IN VARCHAR2,
    EMAIL        IN VARCHAR2,
    COMPANY      IN VARCHAR2,
    TITLE        IN VARCHAR2,
    DISPLAY_RANK IN NUMBER)
AS
BEGIN
  UPDATE REFERENCES
  SET F_NAME         = F_NAME,
    L_NAME           = L_NAME,
    PHONE_NUMBER     = PHONE_NUMBER,
    ADDRESS          = ADDRESS,
    EMAIL            = EMAIL,
    COMPANY          = COMPANY,
    TITLE            = TITLE,
    DISPLAY_RANK     = DISPLAY_RANK
  WHERE RESUMATOR_ID = RESUMATOR_ID
  AND REFERENCE_ID   = REFERENCE_ID;
EXCEPTION
WHEN OTHERS THEN
  raise_application_error(-20001,'An error was encountered - ' ||SQLCODE ||' -ERROR- ' ||SQLERRM);
END; 

CREATE OR REPLACE
PROCEDURE UPDATE_RESEARCH_PUB(
    RESEARCH_PUB_ID IN VARCHAR2,
    RESUMATOR_ID    IN VARCHAR2,
    TITLE           IN VARCHAR2,
  TYPE              IN VARCHAR2,
  LOCATION          IN VARCHAR2,
  DATE_PRESENTED    IN DATE,
  DISPLAY_RANK      IN NUMBER)
AS
BEGIN
  UPDATE RESEARCH_PUB
  SET TITLE           = TITLE,
    TYPE              = TYPE,
    LOCATION          = LOCATION,
    DATE_PRESENTED    = DATE_PRESENTED,
    DISPLAY_RANK      = DISPLAY_RANK
  WHERE RESUMATOR_ID  = RESUMATOR_ID
  AND RESEARCH_PUB_ID = RESEARCH_PUB_ID;
EXCEPTION
WHEN OTHERS THEN
  raise_application_error(-20001,'An error was encountered - ' ||SQLCODE ||' -ERROR- ' ||SQLERRM);
END; 

CREATE OR REPLACE
PROCEDURE UPDATE_USERSSKILLS(
    SKILL_ID     IN VARCHAR2,
    RESUMATOR_ID IN VARCHAR2,
    DISPLAY_RANK IN NUMBER)
AS
BEGIN
  UPDATE USERSSKILLS
  SET DISPLAY_RANK = DISPLAY_RANK
  WHERE SKILL_ID   = SKILL_ID
  AND RESUMATOR_ID = RESUMATOR_ID;
EXCEPTION
WHEN OTHERS THEN
  raise_application_error(-20001,'An error was encountered - ' ||SQLCODE ||' -ERROR- ' ||SQLERRM);
END; 

CREATE OR REPLACE
PROCEDURE UPDATE_WORK_DESC(
    DESC_ID      IN VARCHAR2,
    WORK_ID      IN VARCHAR2,
    DESCRIPTION  IN VARCHAR2,
    DISPLAY_RANK IN NUMBER)
AS
BEGIN
  UPDATE WORK_DESCRIPTION
  SET DESCRIPTION = DESCRIPTION,
    DISPLAY_RANK  = DISPLAY_RANK
  WHERE WORK_ID   = WORK_ID
  AND DESC_ID     = DESC_ID;
EXCEPTION
WHEN OTHERS THEN
  raise_application_error(-20001,'An error was encountered - ' ||SQLCODE ||' -ERROR- ' ||SQLERRM);
END; 

CREATE OR REPLACE
PROCEDURE UPDATE_WORK_EXPERIENCE(
    WORK_ID        IN VARCHAR2,
    RESUMATOR_ID   IN VARCHAR2,
    START_DATE     IN DATE,
    END_DATE       IN DATE,
    CITY           IN VARCHAR2,
    STATE          IN VARCHAR2,
    EMPLOYER       IN VARCHAR2,
    POSITION_TITLE IN VARCHAR2,
    DISPLAY_RANK   IN NUMBER)
AS
BEGIN
  UPDATE WORK_EXPERIENCE
  SET START_DATE     = START_DATE,
    END_DATE         = END_DATE,
    CITY             = CITY,
    STATE            = STATE,
    EMPLOYER         = EMPLOYER,
    POSITION_TITLE   = POSITION_TITLE,
    DISPLAY_RANK     = DISPLAY_RANK
  WHERE RESUMATOR_ID = RESUMATOR_ID
  AND WORK_ID        = WORK_ID;
EXCEPTION
WHEN OTHERS THEN
  raise_application_error(-20001,'An error was encountered - ' ||SQLCODE ||' -ERROR- ' ||SQLERRM);
END; 
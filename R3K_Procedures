CREATE OR REPLACE
PROCEDURE ADD_AWARDS(
    RESUMATOR_ID IN VARCHAR2,
    HONOR_NAME   IN VARCHAR2,
    YEAR         IN DATE,
    SEMESTER     IN VARCHAR2,
    DISPLAY_RANK IN NUMBER)
AS
BEGIN
  INSERT
  INTO AWARD VALUES
    (
      HONOR_ID_SEQ.NEXTVAL,
      RESUMATOR_ID,
      HONOR_NAME,
      YEAR,
      SEMESTER,
      DISPLAY_RANK
    );
EXCEPTION
WHEN OTHERS THEN
  raise_application_error(-20001,'An error was encountered - ' ||SQLCODE ||' -ERROR- ' ||SQLERRM);
END;


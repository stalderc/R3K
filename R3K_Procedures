CREATE OR REPLACE
PROCEDURE UPDATE_AWARDS(
    HONOR_ID     IN VARCHAR2,
    RESUMATOR_ID IN VARCHAR2,
    HONOR_NAME   IN VARCHAR2,
    YEAR         IN DATE,
    SEMESTER     IN VARCHAR2,
    DISPLAY_RANK IN NUMBER)
AS
BEGIN
  UPDATE AWARD
  SET HONOR_NAME   = HONOR_NAME,
    YEAR           = YEAR,
    SEMESTER       = SEMESTER,
    DISPLAY_RANK   = DISPLAY_RANK
  WHERE HONOR_ID   = HONOR_ID
  AND RESUMATOR_ID = RESUMATOR_ID;
EXCEPTION
WHEN OTHERS THEN
  raise_application_error(-20001,'An error was encountered - ' ||SQLCODE ||' -ERROR- ' ||SQLERRM);
END;

CREATE OR REPLACE
PROCEDURE UPDATE_CERTIFICATIONS(
    CERT_ID      IN VARCHAR2,
    RESUMATOR_ID IN VARCHAR2,
    TITLE        IN VARCHAR2,
    DATE_REC     IN DATE,
    DISPLAY_RANK IN NUMBER)
AS
BEGIN
  UPDATE CERTIFICATIONS
  SET TITLE          = TITLE,
    DATE_REC         = DATE_REC,
    DISPLAY_RANK     = DISPLAY_RANK
  WHERE RESUMATOR_ID = RESUMATOR_ID
  AND CERT_ID        = CERT_ID;
EXCEPTION
WHEN OTHERS THEN
  raise_application_error(-20001,'An error was encountered - ' ||SQLCODE ||' -ERROR- ' ||SQLERRM);
END; 

CREATE OR REPLACE
PROCEDURE UPDATE_COMMENTS(
    COMMENTS_ID   IN VARCHAR2,
    RESUMATOR_ID  IN VARCHAR2,
    EMAIL_ADDRESS IN VARCHAR,
    COMMENTS      IN VARCHAR2,
    COMMENTS_DATE IN DATE,
    DISPLAY_RANK  IN NUMBER)
AS
BEGIN
  UPDATE COMMENTS
  SET EMAIL_ADDRESS  = EMAIL_ADDRESS,
    COMMENTS         = COMMENTS,
    COMMENTS_DATE    = COMMENTS_DATE,
    DISPLAY_RANK     = DISPLAY_RANK
  WHERE RESUMATOR_ID = RESUMATOR_ID
  AND COMMENTS_ID    = COMMENTS_ID;
EXCEPTION
WHEN OTHERS THEN
  raise_application_error(-20001,'An error was encountered - ' ||SQLCODE ||' -ERROR- ' ||SQLERRM);
END; 

CREATE OR REPLACE
PROCEDURE UPDATE_COURSE_WORK(
    COURSE_ID    IN VARCHAR2,
    RESUMATOR_ID IN VARCHAR2,
    COURSE_TITLE IN VARCHAR2,
    COURSE_NUMB  IN VARCHAR2,
    SCHOOL_ID    IN VARCHAR2,
    DISPLAY_RANK IN NUMBER)
AS
BEGIN
  UPDATE COURSE_WORK
  SET COURSE_TITLE   = COURSE_TITLE,
    COURSE_NUMBER    = COURSE_NUMBER,
    DISPLAY_RANK     = DISPLAY_RANK
  WHERE RESUMATOR_ID = RESUMATOR_ID
  AND SCHOOL_ID      = SCHOOL_ID
  AND COURSE_ID      = COURSE_ID;
EXCEPTION
WHEN OTHERS THEN
  raise_application_error(-20001,'An error was encountered - ' ||SQLCODE ||' -ERROR- ' ||SQLERRM);
END;

CREATE OR REPLACE
PROCEDURE UPDATE_EDUCATION(
    ED_ID          IN VARCHAR2,
    RESUMATOR_ID   IN VARCHAR2,
    DEGREE         IN VARCHAR2,
    MAJOR          IN VARCHAR2,
    MINOR          IN VARCHAR2,
    CERTIFICATIONS IN VARCHAR2,
    GPA            IN VARCHAR2,
    GRAD_DATE      IN DATE,
    SCHOOL_ID      IN VARCHAR2,
    DISPLAY_RANK   IN NUMBER)
AS
BEGIN
  UPDATE EDUCATION
  SET DEGREE         = DEGREE,
    MAJOR            = MAJOR,
    MINOR            = MINOR,
    CERTIFICATIONS   = CERTIFICATIONS,
    GPA              = GPA,
    GRAD_DATE        = GRAD_DATE,
    DISPLAY_RANK     = DISPLAY_RANK
  WHERE RESUMATOR_ID = RESUMATOR_ID
  AND SCHOOL_ID      = SCHOOL_ID
  AND ED_ID          = ED_ID;
EXCEPTION
WHEN OTHERS THEN
  raise_application_error(-20001,'An error was encountered - ' ||SQLCODE ||' -ERROR- ' ||SQLERRM);
END; 

CREATE OR REPLACE
PROCEDURE UPDATE_EX_CUR_DESC(
    EX_CUR_DESC_ID IN VARCHAR2,
    EX_CUR_ID      IN VARCHAR2,
    DESCRIPTION    IN VARCHAR2,
    DISPLAY_RANK   IN NUMBER)
AS
BEGIN
  UPDATE EXTRA_CURRICULAR_DESCRIPTION
  SET DESCRIPTION    = DESCRIPTION,
    DISPLAY_RANK     = DISPLAY_RANK

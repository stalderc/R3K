  AND EX_CUR_DESC_ID = EX_CUR_DESC_ID;
EXCEPTION
WHEN OTHERS THEN
  raise_application_error(-20001,'An error was encountered - ' ||SQLCODE ||' -ERROR- ' ||SQLERRM);
END;

CREATE OR REPLACE
PROCEDURE UPDATE_EXTRA_CURRICULAR(
    EX_CUR_ID    IN VARCHAR2,
    RESUMATOR_ID IN VARCHAR2,
    TITLE        IN VARCHAR2,
    YEAR         IN VARCHAR2,
    SEMESTER     IN VARCHAR2,
    ORGANIZATION IN VARCHAR2,
    DISPLAY_RANK IN NUMBER)
AS
BEGIN

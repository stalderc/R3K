DROP TABLE PROJECT_DESCRIPTION;
DROP TABLE WORK_DESCRIPTION;
DROP TABLE WORK_EXPERIENCE;
DROP TABLE PASSWORDS;
DROP TABLE AWARD;
DROP TABLE EXTRA_CURRICULAR_DESCRIPTION;
DROP TABLE EXTRA_CURRICULAR;
DROP TABLE USERSSKILLS;
DROP TABLE PROJECTS;
DROP TABLE SKILLS;
DROP TABLE COMMENTS;
DROP TABLE REFERENCES;
DROP TABLE COURSE_WORK;
DROP TABLE EDUCATION;
DROP TABLE SCHOOL;
DROP TABLE RESEARCH_PUB;
DROP TABLE CERTIFICATIONS;
DROP TABLE USERS;
CREATE TABLE USERS
  (
    EMAIL_ADDRESS VARCHAR(50),
    RESUMATOR_ID  VARCHAR(50),
    F_NAME        VARCHAR(140),
    L_NAME        VARCHAR(140),
    STREET        VARCHAR(140),
    CITY          VARCHAR(140),
    STATE         VARCHAR(2),
    ZIPCODE       NUMBER(5),
    PHONE_NUMBER  VARCHAR(10),
    ACCOUNT_TYPE  VARCHAR(140),
    CONSTRAINT EMAIL_PK PRIMARY KEY(EMAIL_ADDRESS),
    CONSTRAINT RES_ID_UNIQUE (RESUMATOR_ID)

  );
CREATE TABLE WORK_EXPERIENCE
  (
    WORK_ID        VARCHAR(50),
    RESUMATOR_ID   VARCHAR(50),
    START_DATE     DATE,
    END_DATE       DATE,
    CITY           VARCHAR(140),
    STATE          VARCHAR(140),
    EMPLOYER       VARCHAR(140),
    POSITION_TITLE VARCHAR(140),
    DISPLAY_RANK   NUMBER(2),
    CONSTRAINT WORK_PK PRIMARY KEY(WORK_ID),
    CONSTRAINT WKXP_RESUMATOR_FK FOREIGN KEY(RESUMATOR_ID) REFERENCES USERS
  );
CREATE TABLE WORK_DESCRIPTION
  (
    DESC_ID      VARCHAR(50),
    WORK_ID      VARCHAR(50),
    DESCRIPTION  VARCHAR(140),
    DISPLAY_RANK NUMBER(2),
    CONSTRAINT DESC_PK PRIMARY KEY(DESC_ID),
    CONSTRAINT WORK_FK FOREIGN KEY(WORK_ID) REFERENCES WORK_EXPERIENCE
  );
CREATE TABLE PASSWORDS
  (
    EMAIL_ADD    VARCHAR(140),
    RESUMATOR_ID VARCHAR(50),
    PASSWORD     VARCHAR(140),
    CONSTRAINT PW_EMAIL_PK PRIMARY KEY(EMAIL_ADD),
    CONSTRAINT PW_RESUMATOR_FK FOREIGN KEY(RESUMATOR_ID) REFERENCES USERS
  );
CREATE TABLE AWARD
  (
    HONOR_ID     VARCHAR(50),
    RESUMATOR_ID VARCHAR(50),
    HONOR_NAME   VARCHAR(140),
    YEAR         VARCHAR(5),
    SEMESTER     VARCHAR(7),
    DISPLAY_RANK NUMBER(2),
    CONSTRAINT HONOR_PK PRIMARY KEY(HONOR_ID),
    CONSTRAINT AWD_RESUMATOR_FK FOREIGN KEY(RESUMATOR_ID) REFERENCES USERS
  );
CREATE TABLE EXTRA_CURRICULAR
  (
    EX_CUR_ID    VARCHAR(50),
    RESUMATOR_ID VARCHAR(50),
    TITLE        VARCHAR(140),
    YEAR         VARCHAR(5),
    SEMESTER     VARCHAR(7),
    ORGANIZATION VARCHAR(200),
    DISPLAY_RANK NUMBER(2),
    CONSTRAINT EX_CUR_PK PRIMARY KEY(EX_CUR_ID),
    CONSTRAINT EC_RESUMATOR_FK FOREIGN KEY(RESUMATOR_ID) REFERENCES USERS
  );
CREATE TABLE EXTRA_CURRICULAR_DESCRIPTION
  (
    EX_CUR_DESC_ID VARCHAR(50),
    EX_CUR_ID      VARCHAR(50),
    DESCRIPTION    VARCHAR(140),
    DISPLAY_RANK   NUMBER(2),
    CONSTRAINT EX_CUR_DESC_PK PRIMARY KEY(EX_CUR_DESC_ID),
    CONSTRAINT EX_CUR_DESC_FK FOREIGN KEY(EX_CUR_ID) REFERENCES EXTRA_CURRICULAR
  );
CREATE TABLE SKILLS
  (
    SKILL_ID   VARCHAR(50),
    SKILL_NAME VARCHAR(140),
    SKILL_TYPE VARCHAR(140),
    CONSTRAINT SKILL_PK PRIMARY KEY(SKILL_ID)
  );
CREATE TABLE USERSSKILLS
  (
    SKILL_ID     VARCHAR(50),
    RESUMATOR_ID VARCHAR(50),
    DISPLAY_RANK NUMBER(2),
    CONSTRAINT USERSSKILL_PK PRIMARY KEY(SKILL_ID,RESUMATOR_ID),
    CONSTRAINT USRSKL_RESUMATOR_FK FOREIGN KEY(RESUMATOR_ID) REFERENCES USERS,
    CONSTRAINT USR_SKILL_ID_FK2 FOREIGN KEY(SKILL_ID) REFERENCES SKILLS
  );
CREATE TABLE PROJECTS
  (
    PROJECT_ID   VARCHAR(50),
    RESUMATOR_ID VARCHAR(50),
    PROJECT_NAME VARCHAR(140),
    SKILL_ID     VARCHAR(50),
    DISPLAY_RANK NUMBER(2),
    CONSTRAINT PROJECT_PK PRIMARY KEY(PROJECT_ID),
    CONSTRAINT PRJ_RESUMATOR_FK FOREIGN KEY(RESUMATOR_ID) REFERENCES USERS,
    CONSTRAINT PRJ_RESUMATOR_FK2 FOREIGN KEY(SKILL_ID) REFERENCES SKILLS
  );
CREATE TABLE PROJECT_DESCRIPTION
  (
    PROJ_DESC_ID VARCHAR(50),
    PROJECT_ID   VARCHAR(50),
    DESCRIPTION  VARCHAR(140),
    DISPLAY_RANK NUMBER(2),
    CONSTRAINT PROJ_DESC_PK PRIMARY KEY(PROJ_DESC_ID),
    CONSTRAINT PROJ_DESC_FK FOREIGN KEY(PROJECT_ID) REFERENCES PROJECTS
  );
CREATE TABLE RESEARCH_PUB
  (
    RESEARCH_PUB_ID VARCHAR(50),
    RESUMATOR_ID    VARCHAR(50),
    TITLE           VARCHAR(140),
    TYPE            VARCHAR(140),
    LOCATION        VARCHAR(140),
    DATE_PRESENTED  DATE,
    DISPLAY_RANK    NUMBER(2),
    CONSTRAINT RESEARCH_PUB_PK PRIMARY KEY(RESEARCH_PUB_ID),
    CONSTRAINT RP_RESUMATOR_FK FOREIGN KEY(RESUMATOR_ID) REFERENCES USERS
  );
CREATE TABLE CERTIFICATIONS
  (
    CERT_ID      VARCHAR(50),
    RESUMATOR_ID VARCHAR(50),
    TITLE        VARCHAR(140),
    DATE_REC     DATE,
    DISPLAY_RANK NUMBER(2),
    CONSTRAINT CERT_PK PRIMARY KEY(CERT_ID),
    CONSTRAINT CERT_RESUMATOR_FK FOREIGN KEY(RESUMATOR_ID) REFERENCES USERS
  );
CREATE TABLE COMMENTS
  (
    COMMENTS_ID   VARCHAR(50),
    RESUMATOR_ID  VARCHAR(50),
    EMAIL_ADDRESS VARCHAR2(50),
    COMMENTS      VARCHAR(1500),
    COMMENTS_DATE DATE,
    DISPLAY_RANK  NUMBER(2),
    CONSTRAINT COMMENTS_PK PRIMARY KEY(COMMENTS_ID),
    CONSTRAINT CMNT_RESUMATOR_FK FOREIGN KEY(RESUMATOR_ID) REFERENCES USERS,
    CONSTRAINT CMNT_RESUMATOR_FK2 FOREIGN KEY(EMAIL_ADDRESS) REFERENCES USERS
  );
CREATE TABLE REFERENCES
  (
    REFERENCE_ID VARCHAR(50),
    RESUMATOR_ID VARCHAR(50),
    F_NAME       VARCHAR(140),
    L_NAME       VARCHAR(140),
    PHONE_NUMBER VARCHAR(20),
    ADDRESS      VARCHAR(140),
    EMAIL        VARCHAR(140),
    COMPANY      VARCHAR(140),
    TITLE        VARCHAR(140),
    DISPLAY_RANK NUMBER(2),
    CONSTRAINT REFERENCE_PK PRIMARY KEY(REFERENCE_ID),
    CONSTRAINT REF_RESUMATOR_FK FOREIGN KEY(RESUMATOR_ID) REFERENCES USERS
  );
CREATE TABLE SCHOOL
  (
    SCHOOL_ID    VARCHAR(50),
    SCHOOL_NAME  VARCHAR(140),
    SCHOOL_STATE VARCHAR(2),
    SCHOOL_CITY  VARCHAR(140),
    CONSTRAINT SCHOOL_PK PRIMARY KEY(SCHOOL_ID)
  );
CREATE TABLE EDUCATION
  (
    ED_ID          VARCHAR(50),
    RESUMATOR_ID   VARCHAR(50),
    SCHOOL_ID      VARCHAR(50),
    DEGREE         VARCHAR(140),
    MAJOR          VARCHAR(140),
    MINOR          VARCHAR(140),
    CERTIFICATIONS VARCHAR(140),
    GPA            VARCHAR(5),
    GRAD_DATE      DATE,
    DISPLAY_RANK   NUMBER(2),
    CONSTRAINT ED_PK PRIMARY KEY(ED_ID),
    CONSTRAINT ED_RESUMATOR_FK FOREIGN KEY(RESUMATOR_ID) REFERENCES USERS,
    CONSTRAINT ED_RESUMATOR_FK2 FOREIGN KEY(SCHOOL_ID) REFERENCES SCHOOL
  );
CREATE TABLE COURSE_WORK
  (
    COURSE_ID     VARCHAR(50),
    RESUMATOR_ID  VARCHAR(50),
    SCHOOL_ID     VARCHAR(50),
    COURSE_TITLE  VARCHAR(140),
    COURSE_NUMBER VARCHAR(140),
    COURSE_DESC   VARCHAR(140),
    DISPLAY_RANK  NUMBER(2),
    CONSTRAINT COURSE_PK PRIMARY KEY(COURSE_ID),
    CONSTRAINT CW_RESUMATOR_FK FOREIGN KEY(RESUMATOR_ID) REFERENCES USERS,
    CONSTRAINT SCHOOL_FK FOREIGN KEY(SCHOOL_ID) REFERENCES SCHOOL
  );
